package com.juwon.generic_study;

import java.util.*;

public class ArrList<T> {
	private T[] array;
	private int count = 0;
	
	public boolean add(T t) {
		boolean add = false;
		
		try {
			array[count] = t;
			count++;
		} catch (IndexOutOfBoundsException e) {
			T[] newArray;
			newArray = array.clone() + newArray;
		}
		
		
		return add;
	}
	
	public int size() {
		return list.size();
	}
	
	public T get(int index) {
		T t;
		t = list.get(index);
		return t;
	}
	
	public static void main(String[] args) {
		ArrayList<Integer> list = new ArrayList<>();
	}
}
