package com.juwon.abstract_study;

abstract class Animal implements a {
	public abstract String type();
}

interface a {
	void create();
	void delete();
	void list();
	void update();
	void read();
}

class Dog extends Animal {
	String type = "포유류";
	public String type() { return type; }
}

class Bird extends Animal {
	String type = "조류";
	public String type() { return type; }
}