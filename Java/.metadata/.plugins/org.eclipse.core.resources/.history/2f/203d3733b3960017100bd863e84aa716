package com.juwon.generic_study;

class NewList<T> {
	private Object list[];
	
	public NewList() {
		list = new Object[1];
	}
	public void add(T item) {
		Object tempList[] = new Object[size() + 1];
		for(int i=0; i<list.length; i++) {
			tempList[i] = list[i];
		}
		tempList[list.length] = item;
		list = tempList;
	}
}
