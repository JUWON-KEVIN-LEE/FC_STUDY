package com.juwon.test;

import java.util.*;

public class test3 {
	int sequenceElement(int[] a, int n) {
	    int result = 0;
	    for(int item : a) {
	        result += item;
	    }
	    result = result % 10;
	    int temp = 0;
	    int count = 5;
	    while(count < n) {
	        temp = a[0];
	        a[0] = a[1];
	        a[1] = a[2];
	        a[2] = a[3];
	        a[4] = result;
	        temp = (result + result - temp);
	        if(temp < 0)
	        	result = (10 - temp) % 10;
	        else
	        	result =  temp % 10;
	        count ++;
	    }
	    return result;
	}
	
	public static void main(String[] args) {
		test3 t = new test3();
		int[] a = {7, 5, 4, 4, 8};
		long start = System.currentTimeMillis();
		int result = t.sequenceElement(a, 5200000000);
		long end = System.currentTimeMillis();
		System.out.println(end - start);
	}
}
