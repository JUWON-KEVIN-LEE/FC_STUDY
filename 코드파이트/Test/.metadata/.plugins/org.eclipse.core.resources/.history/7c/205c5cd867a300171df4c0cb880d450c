package com.juwon.test;

import java.util.*;

public class test3 {
	int sequenceElement(int[] a, int n) {
	    int result = (a[0] + a[1] + a[2] + a[3] + a[4]);
	    int namugi = result % 10;
		
	    int count = 5;
	    while(count < n+1) {
	    	result = result + namugi - a[0];
	    	namugi = result % 10;
	    	System.out.println("result = "+ result + " namugi = " + namugi);
	    	a[0] = a[1];
	    	a[1] = a[2];
	    	a[2] = a[3];
	    	a[3] = a[4];
	    	a[4] = namugi;
	    	count++;
	    }
	    return namugi;
	}
	
	public static void main(String[] args) {
		test3 t = new test3();
		int[] a = {7, 5, 4, 4, 8};
		long start = System.currentTimeMillis();
		int result = t.sequenceElement(a, 10);
		System.out.println(result);
		long end = System.currentTimeMillis();
		System.out.println(end - start);
	}
}
