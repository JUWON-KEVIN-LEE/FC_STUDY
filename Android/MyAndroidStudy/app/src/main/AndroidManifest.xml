<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.immymemine.kevin.myandroidstudy">

    <!-- 권한 설정 -->
    <!-- Runtime Permission -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!-- 안해도 되지만... -->
    <!-- System Permission -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".inflation.Inflation"></activity>
        <activity android:name=".inflation.Inflation2" />
        <activity android:name=".inflation.SetParameter" />
        <activity android:name=".layout.GridLayout" />
        <activity android:name=".Output.GradientPractice" />
        <activity android:name=".permission.Permissions" />
        <activity android:name=".Output.DrawingActivity">
        </activity>
        <activity android:name=".Output.BitmapDrawable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>